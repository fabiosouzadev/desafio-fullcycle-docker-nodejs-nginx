<!DOCTYPE html>
<html lang="en">
<head>
  <title>Full Cycle Rocks!</title>
</head>
<body>
    <h1>Full Cycle Rocks!</h1>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>Nome</th>
        </tr>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=data.id %></td>
            <td><%=data.name %></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
</body>
</html>
